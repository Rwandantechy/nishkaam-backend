 <!-- header -->
 <header
 class="fixed top-0 left-0 p-15 z-50 flex items-center justify-between w-full h-16 bg-gray-100 shadow dark:bg-gray-800"
>
 <a href="/admin" class="flex items-center p-2">
   <img
     class="text-gray-500 transition duration-75 dark:text-gray-400 group-hover:text-gray-900 dark:group-hover:text-white"
     src="/images/logo1.png"
     width="100"
     alt="logo"
   />
 </a>
 <div class="ml-64 relative w-1/2">
   <form class="flex items-center mx-auto">
     <div class="relative w-full px-4">
       <div
         class="absolute inset-y-0 start-0 flex items-center ps-3 pointer-events-none"
       ></div>
       <input
         type="text"
         id="simple-search"
         class="bg-gray-200 border pl-4 border-gray-600 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full ps-10 p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500"
         placeholder="Search Anything..."
       />
     </div>
     <button
       type="submit"
       class="p-3 ms-2 text-sm font-medium text-white bg-blue-700 rounded-lg border border-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800"
     >
       <svg
         class="w-4 h-4"
         aria-hidden="true"
         xmlns="http://www.w3.org/2000/svg"
         fill="none"
         viewBox="0 0 20 20"
       >
         <path
           stroke="currentColor"
           stroke-linecap="round"
           stroke-linejoin="round"
           stroke-width="2"
           d="m19 19-4-4m0-7A7 7 0 1 1 1 8a7 7 0 0 1 14 0Z"
         />
       </svg>
     </button>
   </form>
 </div>
 <!-- user profile -->
 <div class="flex items-center gap-4">
   <button
     class="flex items-center gap-2 p-2 text-gray-900 rounded-lg dark:text-gray-100 hover:bg-gray-200 dark:hover:bg-gray-700"
   >
     <img
       class="w-8 h-8 rounded-full"
       src="/images/profile.png"
       alt="user"
     />
     <span class="text-sm font-medium">Innocent </span>
   </button>
   <button
     class="flex items-center gap-2 p-2 text-gray-900 rounded-lg dark:text-gray-100 hover:bg-gray-200 dark:hover:bg-gray-700"
   >
     <img src="/images/setting.svg" width="32" alt="" />
     <span class="text-sm font-medium">Settings</span>
   </button>
 </div>
</header>